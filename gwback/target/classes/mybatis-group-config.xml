<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
    <setting name="mapUnderscoreToCamelCase" value="true"/>
    <setting name="jdbcTypeForNull" value="NULL" />
  </settings>
  <typeAliases>
	<typeAlias type="com.group.approval.dto.Agreement" alias="agreement"/>
	<typeAlias type="com.group.approval.dto.Approval" alias="approval"/>
	<typeAlias type="com.group.approval.dto.ApprovalStatus" alias="approvalStatus"/>
	<typeAlias type="com.group.approval.dto.Document" alias="document"/>
	<typeAlias type="com.group.approval.dto.DocumentType" alias="documentType"/>
	<typeAlias type="com.group.approval.dto.Reference" alias="reference"/>
    <typeAlias type="com.group.employee.dto.Department" alias="department"/>
    <typeAlias type="com.group.employee.dto.Employee" alias="employee"/>
    <typeAlias type="com.group.employee.dto.Job" alias="job"/>
    <typeAlias type="com.group.employee.dto.Leave" alias="leave"/>
    <typeAlias type="com.group.employee.dto.Position" alias="position"/>
  </typeAliases>
  <mappers>
 	<mapper resource="ApprovalSideMapper.xml"></mapper>
 	<mapper resource="ApprovalConfirmMapper.xml"></mapper>
 	<mapper resource="ApprovalWriteMapper.xml"></mapper>
  </mappers>
</configuration>